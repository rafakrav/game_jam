function sonar(x,y)
y =flr(y/8)
x =flr((x-63)/8)
	
	local ts = time()
	local delay = 0
	local x_tbl ={}
	local x_bag ={}


	for k=x,x+15 do add (x_tbl,k) end
	for l in all(x_tbl) do add(x_bag, l) end

	for j = y,y+15 do
		delay+=0.2
--only way to clone table...
	for i in all(x_bag) do
				hit = fget(mget(i,j))==1
				if (hit) then
					add(sonar_tbl,{i=i,j=j,ts=ts+delay})
					del(x_bag,i)
					goto continue
				end
			::continue::
			end
		
		end
	sfx(0)
end

