function bbl_rndr(x,y,spd,flp)

	local _x = flr(x)
	local _y = flr(y)+2

	if spd != 0 and #ptcl_bbl<=4 then

	if (plr.flp == true)  _x+=6
	if (plr.flp == false) _x-=4
	

	add(ptcl_bbl,{x=_x,
                y=_y,
                ogx = _x,
                ogy = _y})
end 

	for bbl in all(ptcl_bbl) do
		prev_bbly = bbl.y
		bbl.y-=0.5
		if(flr(prev_bbly)>flr(bbl.y)) bbl.x+=rnd({-1,1})
		pset(bbl.x, bbl.y,7)
		if (bbl.y < bbl.ogy -5)	del(ptcl_bbl,bbl)
	end
end


