function plr_coll()
	local prb1 = {x=plr.x-7,
																y=plr.y+3}
	local prb2 = {x=plr.x+8,
																y=plr.y+3}

	local prb_tbl = {prb1,
																		prb2
																	}
	local hit = false

	for prb in all(prb_tbl) do
	col = fget(mget(
							flr(prb.x/8),
							flr((prb.y)/8),
							8)) ==1
	pset(prb.x,prb.y,8)
		if(col) hit = true
	end
	if(hit) game_state = "over"
end

