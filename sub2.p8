function sonar_rndr()

	local snr_dur = 0.6
	local clr = 11
	for snr in all(sonar_tbl) do
				if time() > snr.ts	then
					line(snr.i*8,
					snr.j*8,
					snr.i*8+8,
					snr.j*8,
					clr)
	end

--				if(snr.ts+snr_dur/2<time()) clr = 3
				if(snr.ts+snr_dur<time()) del(sonar_tbl,snr)
	end
	
end

