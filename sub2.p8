function surf_rdr_init()
rdr={x=63,
					y=100,
					angl=1,
    r=13}
rdr_lines={}

rdr_gap_tbl={{-4,10},
            {-4,-10},
            {4,-10},
            {4,10},
            {-10,4},
            {-10,-4},
            {10,-4},
