function clr_blip()
	
	for gap in all(rdr_gap_tbl) do
	local x = gap[1]+rdr.x
	local y = gap[2]+rdr.y
		if pget(x - 1, y) == 3 and
	    pget(x + 1, y) == 3 and
	    pget(x, y - 1) == 3 and
	    pget(x, y + 1) == 3 then
	    pset(x,y,3)
	    end
	end
end
